# Mnemo Insurance - Environment Variables Template
# Copy this to .env and fill in your values

# OpenAI API (for embeddings)
OPENAI_API_KEY=sk-...

# Anthropic/Claude API (for extraction and chat)
ANTHROPIC_API_KEY=sk-ant-...

# Supabase
SUPABASE_URL=https://xxxxx.supabase.co
SUPABASE_ANON_KEY=eyJ...
SUPABASE_SERVICE_ROLE_KEY=eyJ...
SUPABASE_JWT_SECRET=your-jwt-secret

# Database (Supabase PostgreSQL)
# Get from Supabase Dashboard > Settings > Database > Connection string
DATABASE_CONNECTION_STRING=Host=db.xxxxx.supabase.co;Database=postgres;Username=postgres;Password=your-password;Port=5432;SSL Mode=Require;Trust Server Certificate=true

# Storage (using Supabase Storage - no extra config needed, uses SUPABASE_URL)
# Create a bucket named 'documents' in Supabase Storage dashboard
STORAGE_BUCKET_NAME=documents

# Azure Document Intelligence (optional - for OCR fallback)
AZURE_DI_ENDPOINT=
AZURE_DI_KEY=
